{
  "name": "got",
  "version": "1.0.0",
  "description": "Game of Thrones Character Explorer - Angular SPA with NgRx state management",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:full": "concurrently \"npm run start:backend\" \"ng serve\"",
    "start:backend": "node backend/server.js",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /got/",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "docker:build": "docker build -t got-explorer.",
    "docker:run": "docker run -p 4200:4200 got-explorer"
  },
  "keywords": [
    "angular",
    "ngrx",
    "game-of-thrones",
    "spa",
    "typescript",
    "responsive",
    "mobile-first"
  ],
  "author": "Sayomphu",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^20.0.2",
    "@angular/cdk": "^20.0.2",
    "@angular/common": "^20.0.2",
    "@angular/compiler": "^20.0.2",
    "@angular/core": "^20.0.2",
    "@angular/forms": "^20.0.2",
    "@angular/material": "^20.0.2",
    "@angular/platform-browser": "^20.0.2",
    "@angular/platform-browser-dynamic": "^20.0.2",
    "@angular/router": "^20.0.2",
    "@ngrx/effects": "^19.2.1",
    "@ngrx/store": "^19.2.1",
    "@ngrx/store-devtools": "^19.2.1",
    "angular-eslint": "^20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.2",
    "@eslint-recommended/eslint-config": "^28.0.0",
    "@ngrx/schematics": "^19.2.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "concurrently": "^9.1.2",
    "cypress": "^14.4.1",
    "eslint": "^9.30.0",
    "jasmine-core": "^5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.10.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
